#ifndef _HANDLER_CEU
#define _HANDLER_CEU

#include <c.ceu>
#include <uv/uv.ceu>

#include "amqp_base.ceu"

code/await Handler (var _amqp_envelope_t message) -> void do
    _printf("Received message for ctag `%s`.\n", _stringify_bytes(message.consumer_tag));
    _amqp_dump(message.message.body.bytes, message.message.body.len);
end

#endif
